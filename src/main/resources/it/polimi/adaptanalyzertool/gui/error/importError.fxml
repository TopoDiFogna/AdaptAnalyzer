<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="TOP_CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="it.polimi.adaptanalyzertool.gui.error.ImportErrorController">
    <Label text="Json File not valid!">
        <graphic>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../../../../images/error.png"/>
            </ImageView>
        </graphic>
    </Label>
    <Button mnemonicParsing="false" onMouseClicked="#okClicked" text="Ok"/>

</VBox>
