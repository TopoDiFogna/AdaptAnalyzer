<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="it.polimi.adaptanalyzertool.gui.progress.GenericProgressWindowController">
    <Label fx:id="genericProgressMessage">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
        <graphic>
            <ProgressIndicator fx:id="progressIndicator"/>
        </graphic>
    </Label>
    <Button fx:id="okButton" disable="true" mnemonicParsing="false" onMouseClicked="#okClicked" text="Ok"/>

</VBox>
